{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "ArenalApiModel/bin/Release/netstandard2.0/*.dll"
          ],
          "src": "../"
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": true,
      "disableDefaultFilter": false,
      "noRestore": true,
      "namespaceLayout": "nested",
      "memberLayout": "samePage",
      "allowCompilationErrors": false,
      "shouldSkipMarkup": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "about/**.md",
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Arenal Client",
      "_appName": "Arenal Client",
      "_disableContribution": true
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}